{
  "name": "sflightning/lib",
  "type": "library",
  "description": "A library to use Swoole with Symfony. Also an implementation of sflightning/contracts",
  "license": "MIT",
  "authors": [
    {
      "name": "Baptiste CONTRERAS",
      "homepage": "https://github.com/BaptisteContreras"
    }
  ],
  "require": {
    "php": ">=7.3",
    "sflightning/contracts": "dev-main",
    "symfony/event-dispatcher": "^5.4"
  },
  "require-dev": {
    "swoole/ide-helper": "^4.6"
  },
  "conflict": {
    "ext-swoole": "<4.6.0"
  },
  "autoload": {
    "psr-4": {
      "Sflightning\\Lib\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
